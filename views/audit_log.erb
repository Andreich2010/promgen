<div class="page-header">
    <h1>Audit Log</h1>
</div>

<div class="row">
  <table class="table table-bordered">
    <tr>
      <th>Entry</th>
      <th>Timestamp</th>
    <tr />
<% @logs.each do |log| %>
    <tr>
      <td><%= log[:entry] %></td>
      <td><%= DateTime.strptime(log[:timestamp].to_s,'%s') %></td>
    </tr>
<% end %>
  </table>

  <%== will_paginate(@logs, params) %>
</div>
