<div class="page-header">
    <h1>Alert Log</h1>
</div>

<div class="row">
  <table class="table table-bordered">
    <tr>
      <th>Entry</th>
      <th>Timestamp</th>
    <tr />
<% @logs.each do |log| %>
    <tr>
      <td><%= log.entry %></td>
      <td><%= log.timestamp %></td>
    </tr>
<% end %>
  </table>
</div>
